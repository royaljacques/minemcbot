// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
  discordId String @id
  pseudo    String @unique
  language  String
  mana      Int
  manaMax   Int
  xp        Int
  xpMax     Int
  level     Int
  health    Int
  healthMax Int
  power     Int
}
