// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
  discordId String @id
  pseudo String
  lang String
  xp Int
  level Int
  map_name String
  itemInHand Int
}
